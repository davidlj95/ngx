  },
  "packageManager": "pnpm@9.6.0",
  "private": true,
  "devDependencies": {
    "@cypress/code-coverage": "3.12.44",
    "cypress": "13.13.1",
    "tslib": "2.6.3",
    "typescript": "5.5.3"
  }
}
